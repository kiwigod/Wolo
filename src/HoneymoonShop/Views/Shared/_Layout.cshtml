<!DOCTYPE html>
<html lang="nl">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA_Compatible" content="IE=Edge">
    <meta name="viewport" content="width=device-width, inital-scale=1">
    <link href="~/css/bootstrap.min.css" rel="stylesheet">
    <script src="~/js/jquery-3.1.1.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <link href="~/css/style.css" rel="stylesheet">
    <link href="~/css/header.css" rel="stylesheet">
    <link href="~/css/footer.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Cookie|Lato" rel="stylesheet">
    <title>@ViewData["Title"] - HoneymoonShop</title>
    @RenderSection("Imports", required: false)
</head>
<body>


    <div class="container" style="background:@ViewData["Background"]; height:@ViewData["HeaderHeight"]; background-size: 100% 100%;">
        <div class="container-fluid navbar-pink" style="padding:0; margin-left: -1%;">
            <div class="row disappear-xs" style="height:100%">
                <div class="max-height-center col-lg-1 col-md-2 col-sm-2 col-xs-12" style="margin-top: 1.5em; margin-bottom: 1.5em; padding-top:0%;"><a href="#"><b id="website">WEBSITE</b></a></div>
                <div class="max-height-center col-lg-1 col-md-1 col-sm-1 col-xs-12" style="margin-top: 1.5em; margin-bottom: 1.5em; padding-top:0%;"><a href="#" style="color:#f0577a"><b id="webshop">WEBSHOP</b></a></div>
                <div class="max-height-center col-lg-4 col-md-4 col-sm-2 col-xs-12 nopad-xs"></div>
                <div id="tel" class="max-height-center col-lg-offset-1 col-lg-2 col-md-2 col-sm-3 col-xs-12" style="text-align: right; margin-top: 1.5em; margin-bottom: 1.5em; padding-top:0%;"><b>T</b>(010) 4126143</div>
                <div class="col-lg-2 col-md-3 col-sm-4 col-xs-12" style="background: white; width: 270px; float: right; padding-top: 0.67em; padding-bottom: .67em;">
                    <div class="input-group search">
                        <input id="searchfield" type="text" class="form-control" placeholder="Wat zoek je?" style="height: 100%;margin-top: .5em;"> <span class="input-group-btn">
                            <button id="searchbutton" class="btn btn-default primary-button" type="button" style="background-color: white; height:3.1em; width: 3.1em"> <span class="glyphicon glyphicon-search" aria-hidden="true" style="color: black"></span> </button>
                        </span>
                    </div>
                </div>
            </div>
        </div>
        <div class="dropdown show-sm">
            <a asp-controller="Home" asp-action="Index" id="dropdownMenu" class="btn btn-default dropdown-toggle primary-button index-button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true" style="background:none"> <b>HONEYMOON SHOP</b> <span class="caret"></span> </a>
            <ul class="dropdown-menu tablet-fsize" style="background:none" aria-labelledby="dropdown">
                <li><a style="color: #f0577a" href="#"><b>HOME</b></a></li>
                <li><a asp-controller="Dresses" asp-action="Overview"><b>BRUID</b></a></li>
                <li><a href="#"><b>BRUIDEGOM</b></a></li>
                <li><a href="#"><b>DE WINKEL</b></a></li>
                <li><a href="#"><b>BLOG</b></a></li>
                <li><a href="#"><b>CONTACT</b></a></li>
                <li><a href="#"><b>AFSPRAAK MAKEN</b></a></li>
            </ul>
        </div>
        <div class="container-fluid" style="padding: 0; padding-top: 1em">
            <nav class="navbar navbar-default navbar-transparent">
                <div class="container-fluid hide-sm">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#collapseNav"> <span class="sr-only">Toggle Navigation</span> <span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span> </button> <a class="navbar-brand" asp-controller="Home" asp-action="Index"><b>HONEYMOON SHOP</b></a>
                    </div>
                    <div class="collapse navbar-collapse" id="collapseNav">
                        <ul class="nav navbar-nav navbar-right thinner-xs scale-nav">
                            <li><a style="color: #f0577a" href="#"><b>HOME</b></a></li>
                            <li><a asp-controller="Dresses" asp-action="Overview"><b>BRUID</b></a></li>
                            <li><a href="#"><b>BRUIDEGOM</b></a></li>
                            <li><a href="#"><b>DE WINKEL</b></a></li>
                            <li><a href="#"><b>BLOG</b></a></li>
                            <li><a href="#"><b>CONTACT</b></a></li>
                            <li>
                                <form class="navbar-form navbar-left">
                                    <div class="input-group">
                                        <button type="button" class="btn btn-default primary-button"><b>AFSPRAAK MAKEN</b></button>
                                    </div>
                                </form>
                            </li>
                        </ul>
                        <ul class="appear-xs nav navbar-nav thinner-xs">
                            <li><a href="#"><b>WEBSITE</b></a></li>
                            <li><a href="#"><b>WEBSHOP</b></a></li>
                            <li><b>T</b> (010) 4126143 </li>
                            <li>
                                <form class="navbar-form navbar-right" style="margin-top: 0; padding: 0; margin-bottom: 0">
                                    <div class="input-group">
                                        <input type="text" class="form-control" placeholder="Wat zoek je?" style="height: 3.1em;"> <span class="input-group-btn">
                                            <button class="btn btn-default primary-button" type="button" style="background-color: white; height:3.1em; width: 3.1em"> <span class="glyphicon glyphicon-search" aria-hidden="true" style="color: black"></span> </button>

                                        </span>
                                    </div>
                                </form>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
        </div>
        @RenderSection("HeaderOverlay", required: false)
    </div>
    @RenderBody()
    <hr>
    <footer>
        <br> <br> <br>
        <div class="row fullrow1 container">
            <a id="link-pasafspraak" href="http://google.nl">
                <div class="col-md-4 txt-1" id="footer-box-1">
                    <p id="txt-1">Afspraak</p>
                    <p id="txt-1-1">  <b>MAAK EEN <br>PASAFSPRAAK</b></p>
                </div>
            </a>
            <a id="link-social" href="http://google.nl">
                <div class="col-md-4" id="footer-box-2">
                    <p id="txt-2">Social</p>
                    <p id="txt-2-1">  <b>VOLG ONS</b></p>
                    <img src="~/images/web/deel-dit.png" width="214" height="57" border="0" usemap="#map" id="deel-dit" class="img-responsive" />
                    <map name="map">
                        <area shape="rect" coords="1,1,46,57" alt="facebook" href="http://facebook.com" />
                        <area shape="rect" coords="47,2,91,55" alt="twitter" href="http://twitter.com" />
                        <area shape="rect" coords="93,1,131,55" alt="pinterest" href="http://pinterest.com" />
                        <area shape="rect" coords="132,2,167,57" alt="youtube" href="http://youtube.com" />
                        <area shape="rect" coords="167,1,213,58" alt="instagram" href="http://instagram.com" />
                    </map>
                </div>
            </a>
            <a id="link-review" href="http://google.nl">
